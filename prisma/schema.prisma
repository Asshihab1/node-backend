model Product {
  id          Int      @id @default(autoincrement())
  name        String
  price       Float
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  description String?
  category    String?

  @@map("products")
}

model Order {
  id        Int      @id @default(autoincrement())
  productId Int
  quantity  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("orders")
}
